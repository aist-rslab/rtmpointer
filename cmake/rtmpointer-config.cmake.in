# rtmpointer CMake config file
#
# This file sets the following variables:
# RTMPOINTER_FOUND - Always TRUE.
# RTMPOINTER_INCLUDE_DIRS - Directories containing the OpenRTM-aist include files.
# RTMPOINTER_LIBRARIES - Libraries needed to use OpenRTM-aist.
# RTMPOINTER_DEFINITIONS - Compiler flags for OpenRTM-aist.
# RTMPOINTER_VERSION - The version of OpenRTM-aist found.
# RTMPOINTER_VERSION_MAJOR - The major version of OpenRTM-aist found.
# RTMPOINTER_VERSION_MINOR - The minor version of OpenRTM-aist found.
# RTMPOINTER_VERSION_PATCH - The revision version of OpenRTM-aist found.
# RTMPOINTER_VERSION_CAN - The candidate version of OpenRTM-aist found.

message(STATUS "Found rtmpointer-@RTMPOINTER_VERSION@")
set(RTMPOINTER_FOUND TRUE)

set(RTMPOINTER_INCLUDE_DIRS @CMAKE_INSTALL_PREFIX@/include
    @OPENRTM_INCLUDE_DIRS@ @POINTER_INCLUDE_DIRS@)

set(RTMPOINTER_LIBRARIES
    debug
    @CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@rtmpointer@RTMPOINTER_DLLVERSION@@RTMPOINTER_DEBUG_POSTFIX@@CMAKE_SHARED_LIBRARY_SUFFIX@
    optimized
    @CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@rtmpointer@RTMPOINTER_DLLVERSION@@CMAKE_SHARED_LIBRARY_SUFFIX@
    @OPENRTM_LIBRARIES@)

set(RTMPOINTER_DEFINITIONS @OPENRTM_DEFINITIONS@)

set(RTMPOINTER_VERSION @RTMPOINTER_VERSION@)
set(RTMPOINTER_VERSION_MAJOR @RTMPOINTER_VERSION_MAJOR@)
set(RTMPOINTER_VERSION_MINOR @RTMPOINTER_VERSION_MINOR@)
set(RTMPOINTER_VERSION_PATCH @RTMPOINTER_VERSION_PATCH@)
set(RTMPOINTER_VERSION_CAN @RTMPOINTER_VERSION_CAN@)

